
img {

    border-width:0;
    display: block;
    width: 100%;
    height: auto;

    &.cover, &.object-fit {
        width: 100%;
        height: 100%;
        object-fit: cover;
    }

    &.contain {
        width: 100%;
        height: 100%;
        object-fit: contain;
    }

    &.fill {
        width: 100%;
        height: 100%;
        object-fit: fill;
    }
}

